<div>
  <%= form_for @temp_neighborhood, url: signups_path(@temp_neighborhood), html: { method: :post } do |f| %>
    <div class="input-group">
      <div class="field">
        <%= f.label :name %><br>
        <%= f.text_field :name %>
      </div>
      <div class="field">
        <%= f.label :address %><br>
        <%= f.text_field :address %>
      </div>
      <div class="action">
        <%= f.submit %>
      </div>
    </div>
  <% end %>
</div>
