<%= form_tag(signups_create_business_url, multipart: true) do %>
  <div>
    <%= fields_for :temp_business, @temp_business do |f| %>
      <div class="input-group">
        <div class="field">
          <hr>
          <%= f.text_field :name,class:"myPost",placeholder:"Type name here..." %>
        </div>
        <div class="field">
          <hr>
          <%= f.text_field :address,class:"myPost",placeholder:"Type adress here..." %>
        </div>
        <div class="field">
          <hr>
          <%= f.email_field :email,class:"myPost",placeholder:"Type @email here..." %>
          <hr>
        </div>
        <div class="field">
          <%= f.label :image_url, "Credentials" %><br>
          <%= f.file_field :image_url %>
        </div>
      </div>
    <% end %>
  </div>
    <%= submit_tag 'Submit', class: 'btn linky' %>
<% end %>
