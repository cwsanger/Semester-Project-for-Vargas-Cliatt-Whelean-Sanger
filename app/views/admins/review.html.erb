<div class="col-md-6">
  <h1>Users waiting approval</h1>

  <% @temp_users.each do |user| %>

    Name: <%= user.name %><br />
    Email: <%= user.email %>

  <%= button_to 'Accept!!!!', accept_path, params: {:temp_id => user.id, :is_user => 1}, :class => "btn btn-success" %></td>
  
  <hr>

  <% end %>
</div>

<div class="col-md-6">
  <h2>Hoods waiting approval</h2>
  <% @temp_neighborhoods.each do |neighborhood| %>
    Name: <%= neighborhood.name %><br />
    Address: <%= neighborhood.address %><br />
    Contact: <%= neighborhood.contact_email%><br />

  <%= button_to 'Accept!!!!', accept_path, params: {temp_id: neighborhood.id, is_user: false}, :class => "btn btn-success" %></td>
    <hr>
  <% end %>
</div>
