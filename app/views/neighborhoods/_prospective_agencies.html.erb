<table class="table table-hover table-bordered">
  <h1><center>Prospective Agencies</center></h1>
  <hr class="hrPers">
  <tr>
    <th>Name</th>
    <th>Address</th>
    <th>Accept</th>
    <th>Deny</th>
  </tr>
  <% @agencies.each do |agency| %>
    <tr>
      <td><%= agency.name %></td>
      <td><%= agency.address %></td>
      <td><%= link_to '', neighborhood_lead_accept_agency_url(@current_member.neighborhood, agency), method: :post, class:"glyphicon glyphicon-ok accept", remote: true %></td>
      <td><%= link_to '', neighborhood_lead_deny_agency_url(@current_member.neighborhood, agency), method: :post, class:"glyphicon glyphicon glyphicon-remove deny", remote: true%></td>
    </tr>
  <% end %>
</table>
